<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tw.chikuo.idoctor">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".DoctorActivity"  android:label="醫師介紹" android:configChanges="keyboard|keyboardHidden|orientation|screenSize" />
        <activity android:name=".DoctorListActivity"  android:label="醫師列表" android:configChanges="keyboard|keyboardHidden|orientation|screenSize" />
        <activity android:name=".SymptomActivity"  android:label="症狀查詢" android:configChanges="keyboard|keyboardHidden|orientation|screenSize" />

    </application>

</manifest>
